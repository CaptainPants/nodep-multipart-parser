{
    "name": "zerodeps-multipart-parser",
    "version": "0.0.0",
    "type": "module",
    "description": "A zero dependency, high compatibility, multipart HTTP multipart content parser",
    "license": "MIT",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "main": "./dist/cjs/index.js",
    "exports": {
        "import": "./dist/esm/index-module.js",
        "require": "./dist/cjs/index-require.cjs"
    },
    "scripts": {
        "preinstall": "npx only-allow yarn",
        "prepublish": "yarn build",
        "clean": "rm -rf ./dist",
        "build": "yarn run verify && yarn run clean && yarn run build:esm && yarn run build:cjs",
        "build:esm": "tsc --project ./src/lib/tsconfig.json",
        "build:cjs": "tsc --project ./src/lib/tsconfig.json --outDir ./dist/cjs --module commonjs",
        "lint": "npx eslint src/lib/**/*.{ts,js}",
        "lint-fix": "npx eslint src/lib/**/*.{ts,js} --fix",
        "test": "jest --config ./src/lib/jest.config.js",
        "verify": "yarn run lint && yarn run test && yarn run build:esm"
    },
    "dependencies": {
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@jest/globals": "^29.3.1",
        "@types/node": "*",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "eslint": "^8.28.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-react": "^7.31.10",
        "jest": "^29.3.1",
        "terser": "^5.15.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.9.3",
        "typescript-language-server": "^0.11.2"
    }
}
