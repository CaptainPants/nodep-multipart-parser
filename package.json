{
    "name": "@captainpants/zerodeps-multipart-parser",
    "version": "0.0.2",
    "type": "module",
    "description": "A zero dependency, high compatibility, multipart HTTP multipart content parser",
    "license": "MIT",
    "main": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "exports": {
        ".": {
            "default": "./dist/esm/index.js"
        }
    },
    "scripts": {
        "prepublish": "yarn build",
        "clean": "npx rimraf ./dist",
        "build": "yarn run verify && yarn run clean && yarn run compile:esm",
        "compile": "yarn run clean && yarn run compile:esm",
        "compile:esm": "tsc --project ./src/tsconfig.json",
        "compile:cjs": "tsc --project ./src/tsconfig.json --outDir ./dist/cjs/ --module commonjs",
        "lint": "npx eslint src/**/*.{ts,js}",
        "lint-fix": "npx eslint src/**/*.{ts,js} --fix",
        "test": "jest --config ./src/jest.config.js",
        "verify": "yarn run lint && yarn run test"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@jest/globals": "^29.3.1",
        "@types/node": "*",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "eslint": "^8.28.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "rimraf": "^3.0.2",
        "terser": "^5.15.1",
        "ts-jest": "^29.0.3",
        "ts-jest-resolver": "^2.0.0",
        "typescript": "^4.9.3",
        "typescript-language-server": "^0.11.2",
        "yarn": "^1.22.19"
    }
}
