{
    "name": "zerodeps-multipart-parser",
    "version": "0.0.0",
    "type": "module",
    "description": "A zero dependency, high compatibility, multipart HTTP multipart content parser",
    "license": "MIT",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "main": "./dist/amd/index.js",
    "scripts": {
        "preinstall": "npx only-allow yarn",
        "prepublish": "yarn build",
        "clean": "rm -rf ./dist",
        "build": "yarn run clean && yarn run build:esm && yarn run build:umd",
        "build:esm": "tsc",
        "build:umd": "tsc --outDir ./dist/umd --module umd",
        "lint": "npx eslint src/*.{ts,js}",
        "lint-fix": "npx eslint src/*.{ts,js} --fix",
        "test": "jest",
        "verify": "yarn run lint && yarn run test && tsc"
    },
    "dependencies": {
        "@types/node": "*"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@jest/globals": "^29.3.1",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "eslint": "^8.28.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-react": "^7.31.10",
        "jest": "^29.3.1",
        "terser": "^5.15.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.9.3",
        "typescript-language-server": "^0.11.2"
    }
}
