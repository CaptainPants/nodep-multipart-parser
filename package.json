    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.31.10",
    "jest": "^29.3.1",
    "terser": "^5.15.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "typescript-language-server": "^0.11.2"
  }
